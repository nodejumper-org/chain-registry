      },
      {
        "address": "https://ojo-testnet-api.itrocket.net:443",
        "provider": "itrocket"
      },
      {
        "address": "https://ojo-t.api.bccnodes.com",
        "provider": "bccnodes"
      },
      {
        "address": "https://ojo-testnet.api.kjnodes.com",
        "provider": "kjnodes"
      },
      {
        "address": "https://ojo-testnet-api.polkachu.com",
        "provider": "polkachu"
