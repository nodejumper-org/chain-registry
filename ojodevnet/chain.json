      },
      {
        "address": "https://rpc-t.ojo.nodestake.top",
        "provider": "nodestake"
      },
      {
        "address": "https://ojo-testnet-rpc.itrocket.net:443",
        "provider": "itrocket"
      },
      {
        "address": "https://ojo-t.rpc.bccnodes.com",
        "provider": "bccnodes"
      },
      {
        "address": "https://ojo-testnet.rpc.kjnodes.com",
        "provider": "kjnodes"
      },
      {
        "address": "https://archive-rpc.noria.nextnet.zone/",
