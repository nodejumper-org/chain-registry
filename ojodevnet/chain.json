{
  "$schema": "../chain.schema.json",
  "chain_name": "ojodevnet",
  "chain_id": "ojo-devnet",
  "pretty_name": "Ojo",
  "status": "live",
  "network_type": "testnet",
  "bech32_prefix": "ojo",
  "daemon_name": "ojod",
  "node_home": "$HOME/.ojo",
  "key_algos": [
    "secp256k1"
  ],
  "slip44": 118,
  "fees": {
    "fee_tokens": [
      {
        "denom": "uojo"
      }
    ]
  },
  "codebase": {
    "git_repo": "https://github.com/ojo-network/ojo",
