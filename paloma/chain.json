{
  "$schema": "../../chain.schema.json",
  "chain_name": "paloma",
  "chain_id": "messenger",
  "pretty_name": "Paloma",
  "status": "live",
  "network_type": "mainnet",
  "bech32_prefix": "humans",
  "daemon_name": "palomad",
  "node_home": "$HOME/.paloma",
  "key_algos": [
    "secp256k1"
  ],
  "slip44": 118,
  "fees": {
    "fee_tokens": [
      {
        "denom": "ugrain",
        "low_gas_price": 0.01
      }
    ]
  },
  "codebase": {
    "git_repo": "https://github.com/palomachain/paloma",
    "genesis": {
      "genesis_url": "https://raw.githubusercontent.com/palomachain/mainnet/master/messenger/genesis.json"
    }
  },
  "apis": {
    "rpc": [
      {
        "address": "https://rpc-1.paloma.nodes.guru",
        "provider": "nodesguru"
      },
      {
        "address": "https://paloma.rpc.kjnodes.com",
        "provider": "kjnodes"
      }
    ],
    "rest": [
      {
        "address": "https://api-1.paloma.nodes.guru",
        "provider": "nodesguru"
      },
      {
        "address": "https://paloma.api.kjnodes.com",
        "provider": "kjnodes"
      }
    ],
    "grpc": [
    ]
  },
  "explorers": [
    {
      "kind": "nodesguru",
      "url": "https://paloma.explorers.guru/",
      "tx_page": "https://paloma.explorers.guru/transaction/${txHash}"
    }
  ]
}
